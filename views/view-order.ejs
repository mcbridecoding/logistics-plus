<%- include('partials/header') %>

<%- include('partials/nav-panel') %>
<form action="/orders/edit-order-id=<%= order._id %>" method="POST">
<div class="main" id="main">
    <div class="page-title">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-clipboard2" viewBox="0 0 16 16">
            <path d="M3.5 2a.5.5 0 0 0-.5.5v12a.5.5 0 0 0 .5.5h9a.5.5 0 0 0 .5-.5v-12a.5.5 0 0 0-.5-.5H12a.5.5 0 0 1 0-1h.5A1.5 1.5 0 0 1 14 2.5v12a1.5 1.5 0 0 1-1.5 1.5h-9A1.5 1.5 0 0 1 2 14.5v-12A1.5 1.5 0 0 1 3.5 1H4a.5.5 0 0 1 0 1h-.5Z"/>
            <path d="M10 .5a.5.5 0 0 0-.5-.5h-3a.5.5 0 0 0-.5.5.5.5 0 0 1-.5.5.5.5 0 0 0-.5.5V2a.5.5 0 0 0 .5.5h5A.5.5 0 0 0 11 2v-.5a.5.5 0 0 0-.5-.5.5.5 0 0 1-.5-.5Z"/>
        </svg>
        View Order
    </div>
    <div class="new-quote-body">
        <div class="new-quote-menu">
            <div class="quote-sections">
                <div id="shipment-header-button" class="quote-selection" onclick="openShipmentHeader()">Shipment Header</div>
                <div id="shipment-detail-button" class="quote-selection" onclick="openShipmentDetails()">Shipment Details</div>
                <div id="carrier-rates-button" class="quote-selection" onclick="openCarrierRates()">Carrier Rates</div>
                <div id="forms-button" class="quote-selection" onclick="openForms()">Forms</div>
                <div id="shipment-checklist-button" class="quote-selection" onclick="openShipmentChecklist()">Shipment Checklist</div>
                <div id="shipment-costing-button" class="quote-selection" onclick="openShippingCost()">Pricing</div>
            </div>
            <div class="quote-buttons">
                <a href="/logistics/print-logistics-callsheet-id=<%= order._id %>" class="quote-button">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-printer" viewBox="0 0 16 16">
                        <path d="M2.5 8a.5.5 0 1 0 0-1 .5.5 0 0 0 0 1z"/>
                        <path d="M5 1a2 2 0 0 0-2 2v2H2a2 2 0 0 0-2 2v3a2 2 0 0 0 2 2h1v1a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2v-1h1a2 2 0 0 0 2-2V7a2 2 0 0 0-2-2h-1V3a2 2 0 0 0-2-2H5zM4 3a1 1 0 0 1 1-1h6a1 1 0 0 1 1 1v2H4V3zm1 5a2 2 0 0 0-2 2v1H2a1 1 0 0 1-1-1V7a1 1 0 0 1 1-1h12a1 1 0 0 1 1 1v3a1 1 0 0 1-1 1h-1v-1a2 2 0 0 0-2-2H5zm7 2v3a1 1 0 0 1-1 1H5a1 1 0 0 1-1-1v-3a1 1 0 0 1 1-1h6a1 1 0 0 1 1 1z"/>
                    </svg>
                    Print
                </a>
                <button type="submit" class="quote-button">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-save" viewBox="0 0 16 16">
                        <path d="M2 1a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H9.5a1 1 0 0 0-1 1v7.293l2.646-2.647a.5.5 0 0 1 .708.708l-3.5 3.5a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L7.5 9.293V2a2 2 0 0 1 2-2H14a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h2.5a.5.5 0 0 1 0 1H2z"/>
                    </svg>
                    Save & Exit
                </button>
            </div>
        </div>
        <div id="quote-header" class="new-quote-form">
            <div class="flex-container flex-row flex-start gap-25">
                <div class="input-lg">                    
                    <label for="quoteTaker" class="form-label">Name:</label>
                    <input class="form-control" type="text" id="quoteTaker" name="quoteTaker" value="<%= order.quoteTaker %>" required>
                </div>
                <div class="input-sm">
                    <label for="importExport" class="form-label">Import / Export:</label>
                    <select name="importExport" id="importExport" class="form-select" required>
                        <% if (order.importExport === 'import') { %>
                            <option value=""></option>
                            <option value="import" selected>Import</option>
                            <option value="export">Export</option>
                        <% } else if (order.importExport === 'export') { %>
                            <option value=""></option>
                            <option value="import">Import</option>
                            <option value="export" selected>Export</option>
                        <% } else { %>
                            <option value=""></option>
                            <option value="import">Import</option>
                            <option value="export">Export</option>
                        <% } %>
                    </select>
                </div>
                <div class="input-sm">
                    <label for="freightSize" class="form-label">FTL / LTL:</label>
                    <select class="form-select" name="freightSize" id="freightSize">
                        <% if (order.freightSize === 'LTL') { %>
                            <option value=""></option>
                            <option value="LTL" selected>LTL</option>
                            <option value="FTL">FTL</option>
                        <% } else if (order.freightSize ==='FTL') { %>
                            <option value=""></option>
                            <option value="LTL">LTL</option>
                            <option value="FTL" selected>FTL</option>
                        <% } else { %>
                            <option value="" selected></option>
                            <option value="LTL">LTL</option>
                            <option value="FTL">FTL</option>
                        <% } %>
                    </select>
                </div>
                <div class="input-sm">
                    <label for="shipmentType" class="form-label">Type:</label>
                    <select class="form-select" name="shipmentType" id="shipmentType">
                        <% if (order.shipmentType === 'Domestic') { %>
                            <option value=""></option>
                            <option value="Domestic" selected>Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="IntraState">IntraState</option>
                        <% } else if (order.shipmentType === 'Transborder') { %>
                            <option value=""></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder" selected>Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="IntraState">IntraState</option>
                        <% } else if (order.shipmentType === 'Interstate') { %>
                            <option value=""></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate" selected>Interstate</option>
                            <option value="IntraState">IntraState</option>
                        <% } else if (order.shipmentType === 'IntraState') { %>
                            <option value=""></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="IntraState" selected>IntraState</option>
                        <% } else { %>
                            <option value=""selected></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="IntraState">IntraState</option>
                        <% } %>
                    </select>
                </div>
                <div class="input-sm">
                    <label for="quoteDate" class="form-label">Date:</label>
                    <input class="form-control" type="date" id="quoteDate" name="quoteDate" value="<%= order.quoteDate.date %>"> 
                </div>
                <div class="input-sm">
                    <label for="quoteTime" class="form-label">Time:</label>
                    <input class="form-control" type="time" id="quoteTime" name="quoteTime" value="<%= order.quoteDate.time %>"> 
                </div>
            </div>
            <div class="flex-container flex-row gap-50">
                <div class="input-lg">
                    <label for="client" class="form-label">Client / Bill To:</label>
                    <select class="form-select" name="client" id="client" onchange="showAddClientModal(this.value, 'add-client')" required>
                        <option></option>
                        <option value="new">-- Create New --</option>
                        <% addressList.forEach((address) => { %>
                            <% if (address._id == order.client.id) { %>
                                <option value="<%= address._id %>" selected><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                            <% } else {%>
                                <option value="<%= address._id %>"><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                            <% } %>
                        <% }) %>
                    </select>
                </div>
                <div id="add-client-button" class="self-align-end" onclick="openModal('add-client-modal')">
                    <div class="add-address-button">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-three-dots" viewBox="0 0 16 16">
                        <path d="M3 9.5a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"/>
                      </svg>
                    </div>
                </div>
                <div class="input-sm">
                    <label for="quoteNumber" class="form-label">Quote #:</label>
                    <input class="form-control" type="text" id="quoteNumber" name="quoteNumber" value="<%= order.quoteNumber %>" required>  
                </div>
                <div class="input-sm">
                    <label for="paymentTerms" class="form-label">Terms:</label>
                    <select class="form-select" name="paymentTerms" id="paymentTerms" onchange="showThirdpartyInput(this.value)" required>
                        <% if (order.paymentTerms === 'PPD') { %>
                            <option value=""></option>
                            <option value="PPD" selected>Prepaid</option>
                            <option value="COL">Collect</option>
                            <option value="TP">Third Party</option>
                        <% } else if (order.paymentTerms === 'COL') { %>
                            <option value=""></option>
                            <option value="PPD">Prepaid</option>
                            <option value="COL" selected>Collect</option>
                            <option value="TP">Third Party</option>
                        <% } else if (order.paymentTerms === 'TP') { %>
                            <option value=""></option>
                            <option value="PPD">Prepaid</option>
                            <option value="COL">Collect</option>
                            <option value="TP" selected>Third Party</option>
                        <% } else { %>
                            <option value="" selected></option>
                            <option value="PPD">Prepaid</option>
                            <option value="COL">Collect</option>
                            <option value="TP">Third Party</option>
                        <% } %>                      
                    </select>
                </div>
                <div id="thirdPartyInformationPanel" class="input-sm">
                    <label for="thirdPartyInformation" id="" class="form-label">Third Party Bill To:</label>
                    <input class="form-control" type="text" id="thirdPartyInformation" name="thirdPartyInformation" value="<%= order.thirdPartyInformation %>">  
                </div>
                <div class="input-sm">
                    <label for="serviceLevel" class="form-label">Service Level:</label>
                    <input class="form-control" type="text" id="serviceLevel" name="serviceLevel" value="<%= order.serviceLevel %>">
                </div>
            </div>
            <div class="address-information">
                <div class="address-form">
                    <div class="flex-container flex-row gap-50">
                        <div class="input-xl">
                            <label for="shipper" class="form-label">Shipper:</label>
                            <select class="form-select" name="shipper" id="shipper" onchange="showAddClientModal(this.value, 'add-shipper')">
                                <option></option>
                                <option value="new">-- Create New --</option>
                                <% addressList.forEach((address) => { %>
                                    <% if (address._id == order.shipper.id) { %>
                                        <option value="<%= address._id %>" selected><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                                    <% } else {%>
                                        <option value="<%= address._id %>"><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                                    <% } %>
                                <% }) %>
                            </select>
                        </div>
                        <div id="add-shipper-button" class="self-align-end" onclick="openModal('add-shipper-modal')">
                            <div class="add-address-button">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-three-dots" viewBox="0 0 16 16">
                                    <path d="M3 9.5a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"/>
                                </svg>
                            </div>
                        </div>
                    </div>
                    <div id="" class="flex-container flex-row gap-25 shipper-box">
                        <div class="input-sm">
                            <label for="genericShipperCity" class="form-label">City:</label>
                            <input class="form-control" type="text" id="genericShipperCity" name="genericShipperCity" value="<%= order.genericShipper.city %>" readonly>  
                        </div>
                        <div class="input-xs">
                            <label for="genericShipperState" class="form-label">State:</label>
                            <input class="form-control" type="text" id="genericShipperState" name="genericShipperState" value="<%= order.genericShipper.state %>" readonly>  
                        </div>
                        <div class="input-xs">
                            <label for="genericShipperCountry" class="form-label">Country:</label>
                            <input class="form-control" type="text" id="genericShipperCountry" name="genericShipperCountry" value="<%= order.genericShipper.country %>" readonly>  
                        </div>
                    </div>
                    <div class="flex-container flex-row gap-50">
                        <div class="input-xl">
                            <label for="consignee" class="form-label">Consignee:</label>
                            <select class="form-select" name="consignee" id="consignee" onchange="showAddClientModal(this.value, 'add-consignee')">
                                <option value=""></option>
                                <option value="new">-- Create New --</option>
                                <% addressList.forEach((address) => { %>
                                    <% if (address._id == order.consignee.id) { %>
                                        <option value="<%= address._id %>" selected><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                                    <% } else {%>
                                        <option value="<%= address._id %>"><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                                    <% } %>
                                <% }) %>
                            </select>
                        </div>
                        <div id="add-consignee-button" class="self-align-end" onclick="openModal('add-consignee-modal')">
                            <div class="add-address-button">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-three-dots" viewBox="0 0 16 16">
                                <path d="M3 9.5a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"/>
                              </svg>
                            </div>
                        </div>
                    </div>
                    <div id="" class="flex-container flex-row gap-25">
                        <div class="input-sm">
                            <label for="genericConsigneeCity" class="form-label">City:</label>
                            <input class="form-control" type="text" id="genericConsigneeCity" name="genericConsigneeCity" value="<%= order.genericConsignee.city %>" readonly>  
                        </div>
                        <div class="input-xs">
                            <label for="genericConsigneeState" class="form-label">State:</label>
                            <input class="form-control" type="text" id="genericConsigneeState" name="genericConsigneeState" value="<%= order.genericConsignee.state %>" readonly>  
                        </div>
                        <div class="input-xs">
                            <label for="genericConsigneeCounty" class="form-label">Country:</label>
                            <input class="form-control" type="text" id="genericConsigneeCountry" name="genericConsigneeCountry" value="<%= order.genericConsignee.country %>" readonly>  
                        </div>
                    </div>
                </div>
                <div class="address-form">
                    <div class="equipment-checklist">
                        <h4>Equipment Required:</h4>
                        <div class="flex-container flex-row flex-between">
                            <div class="flex-container flex-column flex-start">
                                <div class="form-check">
                                    <% if (order.equipmentRequired.dv) { %>
                                    <input class="form-check-input" type="checkbox" id="DV" name="dv" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="DV" name="dv">
                                    <% } %>
                                    <label class="form-check-label" for="DV">DV - Dry Van</label>
                                </div>
                                <div class="form-check">
                                    <% if (order.equipmentRequired.fd) { %>
                                    <input class="form-check-input" type="checkbox" id="FD" name="fd" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="FD" name="fd">
                                    <% } %>
                                    <label class="form-check-label" for="FD">FD - Flat Deck</label>
                                </div>
                                <div class="form-check">
                                    <% if (order.equipmentRequired.dd) { %>
                                        <input class="form-check-input" type="checkbox" id="DD" name="dd" checked>
                                        <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="DD" name="dd">
                                    <% } %>
                                    <label class="form-check-label" for="DD">DD - Dbl Drop</label>
                                </div>
                            </div>
                            <div class="flex-container flex-column flex-start">
                                <div class="form-check">
                                    <% if (order.equipmentRequired.rf) { %>
                                    <input class="form-check-input" type="checkbox" id="RF" name="rf" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="RF" name="rf">
                                    <% } %>
                                    <label class="form-check-label" for="RF">RF - Reefer Unit</label>
                                </div>
                                <div class="form-check">
                                    <% if (order.equipmentRequired.sd) { %>
                                    <input class="form-check-input" type="checkbox" id="SD" name="sd" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="SD" name="sd">
                                    <% } %>
                                    <label class="form-check-label" for="SD">SD - Step Deck</label>
                                </div>
                                <div class="form-check">
                                    <% if (order.equipmentRequired.fu) { %>
                                    <input class="form-check-input" type="checkbox" id="FU" name="fu" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="FU" name="fu">
                                    <% } %>
                                    <label class="form-check-label" for="FU">FU - Flat Unit</label>
                                </div>
                            </div>
                            <div class="flex-container flex-column flex-start">
                                <div class="form-check">
                                    <% if (order.equipmentRequired.du) { %>
                                    <input class="form-check-input" type="checkbox" id="DU" name="du" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="DU" name="du">
                                    <% } %>
                                    <label class="form-check-label" for="DU">DU - Dry Unit</label>
                                </div>
                                <div class="form-check">
                                    <% if (order.equipmentRequired.sb) { %>
                                    <input class="form-check-input" type="checkbox" id="SB" name="sb" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="SB" name="sb">
                                    <% } %>
                                    <label class="form-check-label" for="SB">SB - Suber B</label>
                                </div>
                                <div class="form-check">
                                    <% if (order.equipmentRequired.rl) { %>
                                    <input class="form-check-input" type="checkbox" id="RL" name="rl" checked>
                                    <% } else { %>
                                        <input class="form-check-input" type="checkbox" id="RL" name="rl">
                                    <% } %>
                                    <label class="form-check-label" for="RL">RL - Rail Unit</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="flex-container flex-row gap-25">
                        <div class="input-md">
                            <label for="sbBroker" class="form-label">SB Broker:</label>
                            <input class="form-control" type="text" id="sbBroker" name="sbBroker" value="<%= order.broker.sb %>">
                        </div>
                        <div class="input-md">
                            <label for="nbBroker" class="form-label">NB Broker:</label>
                            <input class="form-control" type="text" id="nbBroker" name="nbBroker" value="<%= order.broker.nb %>">
                        </div>
                    </div>
                </div>
            </div>
            <div class="flex-container flex-row flex-start gap-50">
                <div class="input-lg">
                    <label for="deliveryEta" class="form-label">ETA:</label>
                    <input class="form-control" type="date" id="deliveryEta" name="deliveryEta" value="<%= order.deliveryEta %>" maxlength="30">
                </div>
                <div class="input-lg">
                    <label for="freightReadyDate" class="form-label">Ready Date / Time:</label>
                    <input class="form-control" type="text" id="freightReadyDate" name="freightReadyDate" value="<%= order.freightReady %>" maxlength="30">
                </div>
                <div class="input-lg">
                    <label for="pickupAppointmentDate" class="form-label">Pick-up Appointment:</label>
                    <input class="form-control" type="text" id="pickupAppointment" name="pickupAppointment" value="<%= order.pickupAppointment %>" maxlength="30">
                </div>
                <div class="input-lg">
                    <label for="deliveryAppointmentDate" class="form-label">Delivery Appointment:</label>
                    <input class="form-control" type="text" id="deliveryAppointment" name="deliveryAppointment" value="<%= order.deliveryAppointment %>" maxlength="30">
                </div>  
            </div>
        </div>
        <div id="carrier-rates" class="new-quote-form">
            <div class="bol-header">
                <a href="/orders/carrier-rates-order-id=<%= order._id %>">+ Add Carrier Rates</a>
            </div>
            <div class="flex-container carrier-rates-table">
                <table class="table table-secondary table-striped table-hover table-bordered">
                    <thead>
                        <tr class="align-center">
                            <th scope="col" class="tbl-fixed-25">#</th>
                            <th scope="col" class="">Name</th>
                            <th scope="col" class="">Carrier</th>
                            <th scope="col" class="">Telephone #</th>
                            <th scope="col" class="">Date</th>
                            <th scope="col" class="">Notes</th>
                            <th scope="col" class="">Truck / Trailer</th>
                            <th scope="col" class="tbl-fixed-50">Asked</th>
                            <th scope="col" class="tbl-fixed-50">Quote</th>
                            <th scope="col" class="tbl-fixed-50">Equip</th>
                            <th scope="col" class="">Currency</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <% order.carrierRates.forEach((rate, index) => { %>
                        <tr class="align-center">
                            <td><%= index + 1 %></td>
                            <td class="no-overflow"><%= rate.dispatchName %></td>
                            <td class="no-overflow"><%= rate.carrierName %></td>
                            <td class="no-overflow"><%= rate.carrierPhone %></td>
                            <td class="no-overflow"><%= rate.date %></td>
                            <td class="align-left comments" onclick="openModal('<%= rate._id %>-comments')"><%= rate.carrierNotes %></td>
                            <td class="no-overflow"><%= rate.carrierTruck %> / <%= rate.carrierTrailer %></td>
                            <td class="no-overflow"><%= rate.asked %></td>
                            <td class="no-overflow"><%= rate.carrierQuoteValue %></td>
                            <td class="no-overflow"><%= rate.carrierEquipment %></td>
                            <td class="no-overflow"><%= rate.carrierQuoteCurrency %></td>
                            <td class="table-options" onclick="openModal('<%= rate._id %>-view')">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                                    <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"/>
                                </svg>
                            </td>
