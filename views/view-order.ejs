<%- include('partials/header') %>

<%- include('partials/nav-panel') %>
<form action="" method="POST">
<div class="main" id="main">
    <div class="page-title">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-clipboard2" viewBox="0 0 16 16">
            <path d="M3.5 2a.5.5 0 0 0-.5.5v12a.5.5 0 0 0 .5.5h9a.5.5 0 0 0 .5-.5v-12a.5.5 0 0 0-.5-.5H12a.5.5 0 0 1 0-1h.5A1.5 1.5 0 0 1 14 2.5v12a1.5 1.5 0 0 1-1.5 1.5h-9A1.5 1.5 0 0 1 2 14.5v-12A1.5 1.5 0 0 1 3.5 1H4a.5.5 0 0 1 0 1h-.5Z"/>
            <path d="M10 .5a.5.5 0 0 0-.5-.5h-3a.5.5 0 0 0-.5.5.5.5 0 0 1-.5.5.5.5 0 0 0-.5.5V2a.5.5 0 0 0 .5.5h5A.5.5 0 0 0 11 2v-.5a.5.5 0 0 0-.5-.5.5.5 0 0 1-.5-.5Z"/>
        </svg>
        View Order
    </div>
    <div class="new-quote-body">
        <div class="new-quote-menu">
            <div class="quote-sections">
                <div id="shipment-header-button" class="quote-selection" onclick="openShipmentHeader()">Shipment Header</div>
                <div id="shipment-detail-button" class="quote-selection" onclick="openShipmentDetails()">Shipment Details</div>
                <div id="shipment-checklist-button" class="quote-selection" onclick="openShipmentChecklist()">Shipment Checklist</div>
                <div id="" class="quote-selection" onclick="">BOL</div>
                <div id="shipment-costing-button" class="quote-selection" onclick="openShippingCost()">Pricing</div>
            </div>
            <div class="quote-buttons">
                <button type="submit" class="quote-button">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-save" viewBox="0 0 16 16">
                        <path d="M2 1a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H9.5a1 1 0 0 0-1 1v7.293l2.646-2.647a.5.5 0 0 1 .708.708l-3.5 3.5a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L7.5 9.293V2a2 2 0 0 1 2-2H14a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h2.5a.5.5 0 0 1 0 1H2z"/>
                    </svg>
                    Save & Exit</button>
            </div>
        </div>
        <div id="quote-header" class="new-quote-form">
            <div class="flex-container flex-row flex-between flex-wrap">
                <div class="">                    
                    <label for="quoteTaker" class="form-label">Name:</label>
                    <input class="form-control input-lg" type="text" id="quoteTaker" name="quoteTaker" value="<%= order.quoteTaker %>">
                </div>
                <div>
                    <label for="freightSize" class="form-label">FTL / LTL:</label>
                    <select class="form-select input-sm" name="freightSize" id="freightSize">
                        <% if (order.freightSize === 'LTL') { %>
                            <option value=""></option>
                            <option value="LTL" selected>LTL</option>
                            <option value="FTL">FTL</option>
                        <% } else if (order.freightSize === 'FTL') { %>
                            <option value=""></option>
                            <option value="LTL">LTL</option>
                            <option value="FTL" selected>FTL</option>
                        <% } else { %>
                            <option value="" selected></option>
                            <option value="LTL">LTL</option>
                            <option value="FTL">FTL</option>
                        <% } %> 
                        <option value=""></option>
                        <option value="LTL">LTL</option>
                        <option value="FTL">FTL</option>
                    </select>
                </div>
                <div>
                    <label for="shipmentType" class="form-label">Type:</label>
                    <select class="form-select input-sm" name="shipmentType" id="shipmentType">
                        <% if (order.shipmentType === 'Domestic') { %>
                            <option value=""></option>
                            <option value="Domestic" selected>Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="Intrastate">Intrastate</option>
                        <% } else if (order.shipmentType === 'Transborder') { %>
                            <option value=""></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder" selected>Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="Intrastate">Intrastate</option>
                        <% } else if (order.shipmentType === 'Interstate') { %>
                            <option value=""></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate" selected>Interstate</option>
                            <option value="Intrastate">Intrastate</option>
                        <% } else if (order.shipmentType === 'IntraState') { %>
                            <option value=""></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="Intrastate" selected>Intrastate</option>
                        <% } else { %>
                            <option value="" selected></option>
                            <option value="Domestic">Domestic</option>
                            <option value="Transborder">Transborder</option>
                            <option value="Interstate">Interstate</option>
                            <option value="Intrastate">Intrastate</option>
                        <% } %>
                    </select>
                </div>
                <div>
                    <label for="quoteDate" class="form-label">Date:</label>
                    <input class="form-control input-sm" type="date" id="quoteDate" name="quoteDate" value="<%= order.quoteDate.date %>"> 
                </div>
                <div>
                    <label for="quoteTime" class="form-label">Time:</label>
                    <input class="form-control input-sm" type="time" id="quoteTime" name="quoteTime" value="<%= order.quoteDate.time %>"> 
                </div>
            </div>
            <div class="flex-container flex-row flex-row flex-wrap gap-50">
                <div>
                    <label for="client" class="form-label">Client / Bill To:</label>
                    <select class="form-select input-lg" name="client" id="client">
                        <% addressList.forEach((address) => { %>
                            <% if (address._id != order.client.id) { %>
                                <option value="<%= address._id %>"><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                            <% } else { %>
                                <option value="<%= address._id %>" selected><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                            <% } %>
                        <% }) %>
                    </select>
                </div>
                <div>
                    <label for="quoteNumber" class="form-label">Quote #:</label>
                    <input class="form-control input-sm" type="text" id="quoteNumber" name="quoteNumber">  
                </div>
                <div>
                    <label for="bol-number" class="form-label">BOL #:</label>
                    <input class="form-control input-sm" type="text" id="bol-number" name="bol-number">  
                </div>
                <div>
                    <label for="paymentTerms" class="form-label">Terms:</label>
                    <select class="form-select input-sm" name="paymentTerms" id="paymentTerms" onchange="showThirdpartyInput(this.value)">
                        <option value=""></option>
                        <option value="PPD">Prepaid</option>
                        <option value="COL">Collect</option>
                        <option value="TP">Third Party</option>
                    </select>
                </div>
                <div id="thirdPartyInformationPanel">
                    <label for="thirdPartyInformation" id="" class="form-label">Third Party Bill To:</label>
                    <input class="form-control input-sm" type="text" id="thirdPartyInformation" name="thirdPartyInformation">  
                </div>
            </div>
            <div class="address-information">
                <div class="address-form">
                    <h2>Shipper:</h2>
                    <div class="flex-container flex-row flex-wrap gap-50">
                        <div>
                            <select class="form-select input-xlg" name="shipper" id="shipper" onchange="openGenericAddress(this.value, 'shipper')">
                                <option></option>
                                <option value="new">-- Create New --</option>
                                <option value="generic">-- Generic --</option>
                                <% addressList.forEach((address) => { %>
                                    <option value="<%= address._id %>"><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                                <% }) %>
                            </select>
                        </div>
                    </div>
                    <div id="generic-shipper" class="flex-container flex-row flex-wrap gap-25">
                        <div>
                            <label for="genericShipperCity" class="form-label">City:</label>
                            <input class="form-control input-sm" type="text" id="genericShipperCity" name="genericShipperCity">  
                        </div>
                        <div>
                            <label for="genericShipperState" class="form-label">State:</label>
                            <input class="form-control input-xs" type="text" id="genericShipperState" name="genericShipperState">  
                        </div>
                        <div>
                            <label for="genericShipperCountry" class="form-label">Country:</label>
                            <input class="form-control input-sm" type="text" id="genericShipperCountry" name="genericShipperCountry">  
                        </div>
                    </div>
                </div>
                <div class="address-form">
                    <h2>Consignee:</h2>
                    <div class="flex-container flex-row flex-wrap gap-50">
                        <div>
                            <select class="form-select input-xlg" name="consignee" id="consignee" onchange="openGenericAddress(this.value, 'consignee')">
                                <option></option>
                                <option value="new">-- Create New --</option>
                                <option value="generic">-- Generic --</option>
                                <% addressList.forEach((address) => { %>
                                    <option value="<%= address._id %>"><%= address.company %> - <%= address.addressOne %>. <%= address.city %>. <%= address.state %></option>
                                <% }) %>
                            </select>
                        </div>
                    </div>
                    <div id="generic-consignee" class="flex-container flex-row flex-wrap gap-25">
                        <div>
                            <label for="genericConsigneeCity" class="form-label">City:</label>
                            <input class="form-control input-sm" type="text" id="genericConsigneeCity" name="genericConsigneeCity">  
                        </div>
                        <div>
                            <label for="genericConsigneeState" class="form-label">State:</label>
                            <input class="form-control input-xs" type="text" id="genericConsigneeState" name="genericConsigneeState">  
                        </div>
                        <div>
                            <label for="genericConsigneeCounty" class="form-label">Country:</label>
                            <input class="form-control input-sm" type="text" id="genericConsigneeCounty" name="genericConsigneeCounty">  
                        </div>
                    </div>
                </div>
            </div>
            <div class="quote-body-split">
                <div class="address-form">
                    <div class="flex-container flex-row flex-wrap gap-50">
                        <div>
                            <label for="sbBroker" class="form-label">SB Broker:</label>
                            <input class="form-control input-lg" type="text" id="sbBroker" name="sbBroker">
                        </div>
                        <div>
                            <label for="nbBroker" class="form-label">NB Broker:</label>
                            <input class="form-control input-lg" type="text" id="nbBroker" name="nbBroker">
                        </div>
                    </div>
                    <h4>Equipment Required:</h4>
                    <div class="equipment-checklist">
                        <div class="flex-container flex-row flex-between">
                            <div class="flex-container flex-column flex-start">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="DV" name="dv">
                                    <label class="form-check-label" for="DV">DV - Dry Van</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="FD" name="fd">
                                    <label class="form-check-label" for="FD">FD - Flat Deck</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="DD" name="dd">
                                    <label class="form-check-label" for="DD">DD - Dbl Drop</label>
                                </div>
                            </div>
                            <div class="flex-container flex-column flex-start">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="RF" name="rf">
                                    <label class="form-check-label" for="RF">RF - Reefer Unit</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="SD" name="sd">
                                    <label class="form-check-label" for="SD">SD - Step Deck</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="FU" name="fu">
                                    <label class="form-check-label" for="FU">FU - Flat Unit</label>
                                </div>
                            </div>
                            <div class="flex-container flex-column flex-start">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="DU" name="du">
                                    <label class="form-check-label" for="DU">DU - Dry Unit</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="SB" name="sb">
                                    <label class="form-check-label" for="SB">SB - Suber B</label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="RL" name="rl">
                                    <label class="form-check-label" for="RL">RL - Rail Unit</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="address-form">
                    <div class="flex-container flex-start flex-wrap gap-50">
                        <div>
                            <label for="freightReadyDate" class="form-label">Ready Date:</label>
                            <input class="form-control input-lg" type="date" id="freightReadyDate" name="freightReadyDate">
                        </div>                        
                        <div>
                            <label for="freightReadyTime" class="form-label">Ready Time:</label>
                            <input class="form-control input-lg" type="time" id="freightReadyTime" name="freightReadyTime">
                        </div>                        
                    </div>
                    <div class="flex-container flex-start flex-wrap gap-50">
                        <div>
                            <label for="serviceLevel" class="form-label">Service Level:</label>
                            <input class="form-control input-lg" type="text" id="serviceLevel" name="serviceLevel">
                        </div>
                        <div>
                            <label for="deliveryEta" class="form-label">ETA:</label>
                            <input class="form-control input-lg" type="date" id="deliveryEta" name="deliveryEta">
                        </div>
                    </div>
                    <div class="flex-container flex-row flex-wrap gap-50">
                        <div>
                            <label for="pickupAppointmentDate" class="form-label">Pick up Appointment:</label>
                            <input class="form-control input-lg" type="date" id="pickupAppointmentDate" name="pickupAppointmentDate">
                        </div>
                        <div>
                            <label for="pickupAppointmentTime" class="form-label">Pick up Appointment Time:</label>
                            <input class="form-control input-lg" type="time" id="pickupAppointmentTime" name="pickupAppointmentTime">
                        </div>
                    </div>
                    <div class="flex-container flex-row flex-wrap gap-50">
                        <div>
                            <label for="deliveryAppointmentDate" class="form-label">Delivery up Appointment:</label>
                            <input class="form-control input-lg" type="date" id="deliveryAppointmentDate" name="deliveryAppointmentDate">
                        </div>
                        <div>
                            <label for="deliveryAppointmentTime" class="form-label">Delivery Appointment Time:</label>
                            <input class="form-control input-lg" type="time" id="deliveryAppointmentTime" name="deliveryAppointmentTime">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="shipment-details" class="new-quote-form">
            <div class="quote-body-split">
                <div class="details-column">
                    <div class="dimension-table">
                        <div class="flex-container flex-between gap-50">
                            <h4>Shipments Dims:</h4>
                            <h4 id="totalCUFT">CUFT: </h4>    
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l1" name="l1" onchange="calculateCUFT(1)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w1" name="w1" onchange="calculateCUFT(1)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h1" name="h1" onchange="calculateCUFT(1)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c1" name="c1" onchange="calculateCUFT(1)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf1" name="cf1" readonly>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l2" name="l2" onchange="calculateCUFT(2)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w2" name="w2" onchange="calculateCUFT(2)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h2" name="h2" onchange="calculateCUFT(2)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c2" name="c2" onchange="calculateCUFT(2)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf2" name="cf2" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l3" name="l3" onchange="calculateCUFT(3)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w3" name="w3" onchange="calculateCUFT(3)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h3" name="h3" onchange="calculateCUFT(3)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c3" name="c3" onchange="calculateCUFT(3)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf3" name="cf3" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l4" name="l4" onchange="calculateCUFT(4)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w4" name="w4" onchange="calculateCUFT(4)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h4" name="h4" onchange="calculateCUFT(4)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c4" name="c4" onchange="calculateCUFT(4)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf4" name="cf4" value="" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l5" name="l5" onchange="calculateCUFT(5)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w5" name="w5" onchange="calculateCUFT(5)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h5" name="h5" onchange="calculateCUFT(5)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c5" name="c5" onchange="calculateCUFT(5)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf5" name="cf5" value="" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l6" name="l6" onchange="calculateCUFT(6)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w6" name="w6" onchange="calculateCUFT(6)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h6" name="h6" onchange="calculateCUFT(6)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c6" name="c6" onchange="calculateCUFT(6)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf6" name="cf6" value="" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l7" name="l7" onchange="calculateCUFT(7)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w7" name="w7" onchange="calculateCUFT(7)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h7" name="h7" onchange="calculateCUFT(7)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c7" name="c7" onchange="calculateCUFT(7)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf7" name="cf7" value="" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l8" name="l8" onchange="calculateCUFT(8)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w8" name="w8" onchange="calculateCUFT(8)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h8" name="h8" onchange="calculateCUFT(8)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c8" name="c8" onchange="calculateCUFT(8)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf8" name="cf8" value="" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l9" name="l9" onchange="calculateCUFT(9)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w9" name="w9" onchange="calculateCUFT(9)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h9" name="h9" onchange="calculateCUFT(9)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c9" name="c9" onchange="calculateCUFT(9)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf9" name="cf9" value="" disabled>
                        </div>
                        <div class="flex-container flex-row gap-10">
                            <input class="form-control input-xs" type="text" placeholder="L" id="l10" name="l10" onchange="calculateCUFT(10)">
                            <input class="form-control input-xs" type="text" placeholder="W" id="w10" name="w10" onchange="calculateCUFT(10)">
                            <input class="form-control input-xs" type="text" placeholder="H" id="h10" name="h10" onchange="calculateCUFT(10)">
                            <input class="form-control input-xs" type="text" placeholder="#" id="c10" name="c10" onchange="calculateCUFT(10)">
                            <input class="form-control input-xs" type="text" placeholder="CF" id="cf10" name="cf10" value="" disabled>
                        </div>
                    </div>
                    <div class="flex-container flex-between flex-wrap gap-50">
                        <div>
                            <label for="dryTemp" class="form-label">Dry / Temp Controlled:</label>
                            <select class="form-select input-lg" name="dryTemp" id="dryTemp">
                                <option value=""></option>
                                <option value="Dry">Dry</option>
                                <option value="Temp">Temp Controlled</option>
                            </select>
                        </div>
                        <div>
                            <label for="temp" class="form-label">Temp:</label>
                            <input class="form-control input-lg" type="text" id="temp" name="temp">
                        </div>
                    </div>
                </div>
                <div class="details-column">
                    <div class="dimension-table">
                        <div class="flex-container flex-row flex-wrap gap-10">
                            <div>
                                <label for="commodity" class="form-label">Commodity:</label>
                                <input class="form-control input-xs" type="text" id="commodity" name="commodity">
                            </div>
                            <div>
                                <label for="totalPieces" class="form-label">Total Pieces:</label>
                                <input class="form-control input-xs" type="text" id="totalPieces" name="totalPieces">
                            </div>
                            <div>
                                <label for="totalWeight" class="form-label">Total Weight:</label>
                                <input class="form-control input-xs" type="text" id="totalWeight" name="totalWeight">
                            </div>
                            <div>
                                <label for="weightMeasurement" class="form-label">Measurement:</label>
