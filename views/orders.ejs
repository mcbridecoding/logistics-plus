                                <input class="form-control input-sm" type="text" placeholder="Shipper City">
                                <input class="form-control input-xs" type="text" placeholder="State / Prov">
                            </div>
                            <div class="flex-container flex-row flex-start gap-10">
                                <input class="form-control input-sm" type="text" placeholder="Consignee City">
                                <input class="form-control input-xs" type="text" placeholder="State / Prov">
                            </div>
                            <button class="search-button">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
